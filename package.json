{
  "name": "pinmypi",
  "version": "1.0.0",
  "description": "PinMyPi is a web service that receives GPS locations and lists them, similar to \"Find My iPhone\", but for your Raspberry Pis (and anything else you want).",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./www.js",
    "migrate-db": "node_modules/.bin/sequelize db:migrate",
    "seed-db": "node_modules/.bin/sequelize db:seed:all"
  },
  "repository": "https://github.com/rGunti/PinMyPi.git",
  "keywords": [
    "pinmypi",
    "gps",
    "raspberry",
    "pi"
  ],
  "author": {
    "name": "Raphael \"rGunti\" Guntersweiler",
    "email": "raphael@rgunti.ch",
    "url": "https://rgunti.ch"
  },
  "license": "MIT",
  "bugs": "https://github.com/rGunti/PinMyPi/issues",
  "homepage": "https://github.com/rGunti/PinMyPi#readme",
  "dependencies": {
    "async": "^2.6.1",
    "body-parser": "1.18.3",
    "bootstrap": "4.1.1",
    "config": "1.30.0",
    "connect-ensure-login": "0.1.1",
    "cookie-parser": "1.4.3",
    "debug": "3.1.0",
    "express": "4.16.3",
    "express-hbs": "1.0.4",
    "express-session": "1.15.6",
    "font-awesome": "4.7.0",
    "handlebars-helpers": "0.10.0",
    "jquery": "3.3.1",
    "js-base64": "2.4.5",
    "moment": "2.22.2",
    "moment-duration-format": "2.2.2",
    "morgan": "1.9.0",
    "mysql2": "1.5.3",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "passport-localapikey": "0.0.3",
    "passport-remember-me": "0.0.1",
    "sequelize": "4.38.0",
    "sequelize-cli": "4.0.0",
